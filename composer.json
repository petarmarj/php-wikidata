{
  "name": "petarmarj/php-wikidata",
  "description": "A PHP client for working with Wikidata API.",
  "keywords": [
    "wikidata",
    "client",
    "php"
  ],
  "homepage": "https://github.com/freearhey/wikidata",
  "license": "MIT",
  "version": "3.6.0",
  "authors": [
    {
      "name": "Arhey",
      "role": "developer"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/collections": "^11.41"
  },
  "autoload": {
    "psr-4": {
      "Wikidata\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Wikidata\\Tests\\": "tests"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "*"
  }
}
